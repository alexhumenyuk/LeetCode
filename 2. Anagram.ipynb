    "class Solution:\n",
    "    def isAnagram(self, s: str, t: str) -> bool:\n",
    "        return sorted(s.lower()) == sorted(t.lower())\n",
    "        s=list(s)\n",
    "        s.sort()\n",
    "        t=list(t)\n",
    "        t.sort()\n",
    "        return s==t\n",
    "s=\"AMOR\"\n",
    "t=\"roma\"\n",
    "sol=Solution()\n",
    "print(sol.isAnagram(s,t))"
   ]
